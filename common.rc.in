1 VERSIONINFO
FILEVERSION ${SDK_VERSION_MAJOR},${SDK_VERSION_MINOR},${SDK_VERSION_PATCH},0
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904B0"
    BEGIN
      VALUE "CompanyName", "${CPF_COMPANY_NAME}"
      VALUE "FileDescription", "${CPF_MODULE_DESCRIPTION}"
      VALUE "FileVersion", "${SDK_VERSION_STR}"
      VALUE "ProductName", "${CPF_PRODUCT_NAME}"
      VALUE "ProductVersion", "${SDK_VERSION_STR}"
      VALUE "Comments", "${CPF_COMMENTS}"
      VALUE "LegalCopyright", "${CPF_LEGAL_COPYRIGHT}"
      VALUE "InternalName", "${PROJECT_NAME}"
      VALUE "OriginalFilename", "${PROJECT_NAME}"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 0x04B0
  END
END